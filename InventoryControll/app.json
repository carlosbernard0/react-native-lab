{
  "expo": {
    "name": "InventoryControll",
    "slug": "InventoryControll",
    "scheme": "InventoryControll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sispro-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/sispro-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/sispro-logo.png",
      "bundler": "metro",
      "manifest": {
        "name": "My PWA App",
        "short_name": "MyApp",
        "description": "This is a PWA made with Expo",
        "display": "standalone",
        "start_url": "/",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./assets/favicon.png",
            "sizes": "192x192",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
